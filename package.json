{
  "name": "feature-toggles",
  "version": "0.1.6",
  "description": "Feature toggles helps to build features, keep code clean and separate. This is a tool for use feature toggling in a REACT web application.",
  "main": "index.js",
  "author": "Sanjay <contact@sanjaypanda.com>",
  "license": "MIT",
  "keywords": [
    "babel-plugin"
  ],
  "scripts": {
    "node:debug": "babel-node ./scripts/run basic --inspect-brk=26341",
    "start": "babel-node ./scripts/run basic",
    "build": "babel src --out-dir lib --copy-files README.md LICENSE && rm -rf ./lib/__test__",
    "test": "jest --toggleName=ft1 --toggleDir=./examples/basic",
    "lint": "eslint .",
    "prelive": "rm -rf lib && yarn run lint && yarn run test && yarn run build && babel-node ./scripts/copyPackage.js --name babel-plugin-feature-toggles  ",
    "live:babel": "npm run prelive && npm publish ./lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pandasanjay/feature-toggles.git"
  },
  "dependencies": {
    "@babel/preset-env": "^7.7.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/plugin-transform-react-jsx": "^7.7.4",
    "@babel/traverse": "^7.7.4",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "debug": "^4.1.1",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "yargs": "^15.0.2"
  }
}
